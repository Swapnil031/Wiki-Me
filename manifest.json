{
	"manifest_version": 2,
	"name": "WikiMe",
	"version": "1.0",
	"description": "Search on WikiMe",
	"icons":{
		"128": "icon128.png",
		"48": "icon48.png",
		"16": "icon16.png"
	},

    "background" : {
    	"scripts": ["eventPage.js"],
    	"persistent": false
    },

	"permissions": [
		"tabs",
		"storage",
		"notifications",
		"contextMenus"
	]
}